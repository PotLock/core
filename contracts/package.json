  "scripts": {
    "build:pot": "cd pot/ && ./scripts/build.sh && cd ../",
    "build:potdeployer": "cd pot_deployer/ && ./scripts/build.sh && cd ../",
    "dev:deploy:pot": "cd pot && (near dev-deploy || exit 0) && cd ..",
    "dev:deploy:pot:refresh": "rm -rf neardev && yarn dev:deploy:pot",
    "dev:deploy:potdeployer": "cd pot_deployer && (near dev-deploy || exit 0) && cd ..",
    "dev:deploy:potdeployer:refresh": "rm -rf neardev && yarn dev:deploy:potdeployer"
  }
